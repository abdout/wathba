  "about": {
    "sections": {
      "who": {
        "subtitle": "W h o   W e   A r e",
        "title": "Who We Are",
        "description": "Established in 2017, Alwathba Cooperative Society was founded with the goal of supporting and developing the local community by providing exceptional cooperative services that enhance the quality of life and meet the local community needs efficiently and transparently. Since our establishment, we have been committed to building a cooperative system based on community values, teamwork, and sustainable economic practices."
      },
      "why": {
        "subtitle": "W h y   U s",
        "title": "Why Us",
        "description": "We believe that cooperation work is the foundation for inclusive development. With our dedication to offering high-quality products and services that reflect the aspirations of the community, we meet the growing needs efficiently. Our commitment to transparency, community values, and sustainable practices makes us the trusted choice for cooperative services in Alwathba."
      },
      "mission": {
        "subtitle": "O u r   M i s s i o n",
        "title": "Our Vision and Mission",
        "description": "To provide an effective cooperative environment that supports individuals, offers reliable products and services, and launches developmental initiatives and strategic partnerships that enhance awareness and living standards. We strive to be the leading cooperative society that promotes the principle of cooperation and joint community work."
      }
    },
    "goals": [
      {
        "title": "Community Support",
        "description": "Building strong community bonds through cooperative initiatives that support individuals and families in achieving their goals."
      },
      {
        "title": "Quality Services",
        "description": "Delivering reliable, high-quality products and services that meet the evolving needs of our community with transparency and efficiency."
      },
      {
        "title": "Sustainable Development",
        "description": "Implementing sustainable economic practices that ensure long-term growth and prosperity for our cooperative society and community."
      }
    ],
